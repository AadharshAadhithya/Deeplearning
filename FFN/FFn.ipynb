    "def softmax(x):\n",
    "    exp_x = np.exp(x)\n",
    "    return ((exp_x)/np.sum(exp_x))\n",
    "\n",
    "def cross_Entropy_loss(y_hat,y):\n",
    "    return np.sum((y*np.log2(y_hat)))\n",
    "        "
